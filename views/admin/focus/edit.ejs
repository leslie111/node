<div class="content">

    <div class="container-fluid">
        <div class="row-fluid">

            轮播图修改
            <form action="<%=host%>admin/focus/doedit" enctype="multipart/form-data" method="post">

                <input type="hidden" name="id" value="<%=list[0]._id%>"/>
                <p>
                    cate_id: <input type="text" name="cate_id" value="<%=list[0].cate_id%>"/>
                </p>
                <p>
                    title: <input type="text" name="title" value="<%=list[0].title%>"/>
                </p>
                <p>
                    img: <input type="file" name="img" multiple="multiple"/>
                </p>

                <p>
                    description: <input type="text" name="description" value="<%=list[0].description%>"/>
                </p>
                <p>
                    <input type="submit" value="修改"/>
                </p>
            </form>

        </div>
    </div>
</div>



<script src="<%=host%>lib/bootstrap/js/bootstrap.js"></script>
