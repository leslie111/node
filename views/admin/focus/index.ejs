<div class="content">

    <div class="container-fluid">
        <div class="row-fluid">

            轮播图模块

            <table border="1" cellpadding="0" cellspacing="0" align="center" width="1000">
                <thead>
                    <th>cate_id</th>
                    <th>title</th>
                    <th>img</th>
                    <th>description</th>
                    <th>edit</th>
                </thead>
                <tbody>
                <%for(var i=0;i<list.length;i++) {%>
                    <tr align="center">
                        <td><%=list[i].cate_id%></td>
                        <td><%=list[i].title%></td>
                        <td>
                            <img src="<%=list[i].img%>" style="width: 60px">
                        </td>
                        <td><%=list[i].description%></td>
                        <td><a href="<%=host%>admin/focus/edit?id=<%=list[i]._id%>">修改</a>　<a href="<%=host%>admin/focus/del?id=<%=list[i]._id%>">删除</a></td>
                    </tr>
                <%}%>
                </tbody>
            </table>
            <div>
                <a href="<%=host%>admin/focus/add">增加轮播图</a>
            </div>


        </div>
    </div>
</div>



<script src="<%=host%>lib/bootstrap/js/bootstrap.js"></script>
<script type="text/javascript">
	$("[rel=tooltip]").tooltip();
	$(function() {
		$('.demo-cancel-click').click(function(){return false;});
	});
</script>

</body>
</html>